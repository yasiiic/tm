<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="myfirstapp.example.com.thomasianmerchandises" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".firstscreen"
            android:label="@string/title_activity_firstscreen" >
        </activity>
        <activity
            android:name=".MyActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".shirt"
            android:label="@string/title_activity_shirt" >
        </activity>
        <activity
            android:name=".hoodie"
            android:label="@string/title_activity_hoodie" >
        </activity>
        <activity
            android:name=".lanyard"
            android:label="@string/title_activity_lanyard" >
        </activity>
        <activity
            android:name=".shirtzoom"
            android:label="@string/title_activity_shirtzoom" >
        </activity>
    </application>

</manifest>
